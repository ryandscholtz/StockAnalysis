# Performance testing environment variables
# Disable heavy middleware and features for load testing

# Disable X-Ray tracing
XRAY_ENABLED=false

# Disable EC2 auto-stop monitoring
EC2_AUTO_STOP=false

# Disable dashboard initialization
INIT_DASHBOARDS_ON_STARTUP=false

# Set minimal logging
LOG_LEVEL=WARNING
STRUCTURED_LOGGING=false

# Disable CloudWatch metrics
CLOUDWATCH_ENABLED=false

# Use minimal database settings
DATABASE_POOL_SIZE=1
DATABASE_TIMEOUT=5